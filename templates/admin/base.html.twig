<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}{% endblock %}</title>
    {# Run `composer require symfony/webpack-encore-bundle`
       and uncomment the following Encore helpers to start using Symfony UX #}
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('assets/framework/bootstrap-5.0.0/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/css/main/main.css') }}">
        {#{{ encore_entry_link_tags('app') }}#}
    {% endblock %}

    {% block javascripts %}
        <script src="{{ asset('assets/framework/bootstrap-5.0.0/js/bootstrap.min.js') }}"></script>
        {#{{ encore_entry_script_tags('app') }}#}
    {% endblock %}
</head>
<body>
{% block body %}

{% endblock %}
</body>
</html>
